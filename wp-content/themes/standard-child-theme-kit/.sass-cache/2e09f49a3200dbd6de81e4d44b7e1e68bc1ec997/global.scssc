:@has_childrenT:@template"÷#/* ------------------------------------------------------------------------ Import Standard Styles */

@import "../../standard/style.css";
@import "font-awesome";
@import "font-awesome-ie7";

@import "compass";
@import "compass/css3";
@import "compass/typography";

@import "variables";

@import "wells";

@include establish-baseline;

/* ------------------------------------------------------------------------ Global */

p:empty {
  display: none;
}

*,
::after,
::before {
  @include box-sizing(border-box);
}

a:hover {
  text-decoration: none;
}

h1 {
  display: inline-block;
  width: 100%;
  font-family: 'PT Sans', 'Helvetica Neue', Helvetica, Arial, sans-serif;
}

body p, h2, h3, h4, h5, h6{
  font-family: 'Open Sans', 'Helvetica Neue', Helvetica, Arial, sans-serif;
}

.post .entry-content,
.post .entry-summary {
  padding: 18px 20px 20px;
}

#film-page h1 {
  border-top: 2px solid $darkgray;
  border-bottom: 2px solid $darkgray;
  padding-left: 10px;
  margin-bottom: $basespace;
}

.awesome {
  @include adjust-font-size-to(30px);
  position: relative;
  top: 3px;
  padding-right:5px;
}

.post-header {
	display: none;
}

.module {
  float: left;
  margin-bottom: 40px;
}

#text-4 h4{
  color: #FFF;
}

.twitter {
  color: $twitter;
}

.wp-caption-text {
  font-family: Menlo, Monaco, "Courier New", monospace;
}

.widget ul li a {
color: $link;
}

.widget ul li a:hover {
  color: $link-hover;
}

/* ------------------------------------------------------------------------ Bootstrap Add-Ons */

.container-fluid {
  padding: 0px;
}

.btn-flat {
  filter:progid:DXImageTransform.Microsoft.Gradient(enabled='false');
  @include box-shadow(0);
  background-image:none;
  border:0;
  @include border-radius(0);
}

/*#film-page .offset1 {
  margin-left: 95px;
}*/

blockquote {
  border-left: 5px solid #7CAACD;
  font-style: italic;
}

.img-rounded {
    -webkit-border-radius: 4px;
      -moz-border-radius: 4px;
        border-radius: 4px;
}

.img-circle {
    -webkit-border-radius: 500px;
      -moz-border-radius: 500px;
        border-radius: 500px;
  margin-bottom: 0px;
}

/* ------------------------------------------------------------------------ Nav */

.nav.social-icons li:nth-child(3) {
  display: none;
}

.navbar .social-icons > li {
  margin: 2px 0 0px 12px;
}

.home-icon {
  float:left;
  padding-top: 2px;
  width: 35px;
  height: 35px;
}

.navbar .nav {
  margin-left: 10px;
}

.spotlight-hr {
  border-top: 1px solid $darkgray;
  margin: 18px 0px;
}

/* ------------------------------------------------------------------------ Home */

.home .marbot {
  margin-bottom: 40px;
}

.home .breadcrumb {
  display: none;
}

/*.home .hero-unit {
  margin: $basespace 0px 30px 0px;
  padding:10px 60px;
  text-align: center;
}

.home .hero-unit h1 {
  @include adjust-font-size-to(49px);
}*/

/*.home h2:first-child{

}*/

.home #wrapper img {
  margin-bottom: $basespace;
}

#spotlight-image {
 margin-bottom: 0px !important;
}

.fademe img,
a img {
  @include opacity(1);
  @include transition(opacity 0s ease-in-out)
}

.fademe img:hover,
a img:hover {
  @include opacity(0.9);;
}

/* ------------------------------------------------------------------------ Sidebar */

.standard-pi-pic {
  padding: 13px;
}
#sidebar .widget-title {
  color: #fff;
}

#mc_embed_signup {
  background:transparent;
  clear:left;
  @include adjust-font-size-to(14px);
  font-family: 'Helvetica-Neue' Helvetica,Arial,sans-serif;
}

#mc_embed_signup form {
  padding: 10px 0 10px 0 !important;
}

#mc_embed_signup input.email {
  width: 100% !important;
  padding: 17px 0px !important;
}

/* ------------------------------------------------------------------------ FAQ */

#faq h3 {
  text-align: center;
  margin-bottom: $basespace;
}

#faq .icon-large {
  display: block;
  line-height: normal;
  height: 50px;
}

#faq .icon-large::before {
  @include adjust-font-size-to(64px);
}

#faq p {
  text-align: center;
}

#faq ul {
  margin: 0px;
}

#faq li {
  list-style: none;
}

.accordion {
  margin-bottom: 18px;
}

.accordion-group {
  margin-bottom: 2px;
  border: 1px solid #e5e5e5;
  @include border-radius (4);
}

.accordion-heading {
  border-bottom: 0;
}

.accordion-heading .accordion-toggle {
  display: block;
  padding: 8px 15px;
}

.accordion-toggle {
  cursor: pointer;
}

.accordion-inner {
  padding: 9px 15px;
  border-top: 1px solid #e5e5e5;
}

/* ------------------------------------------------------------------------ Contact */

#myModal input[type="submit"] {
  text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.25);
  color: #fff;
  background-color: #5BB75B;
/*  background-image: -moz-linear-gradient(top, #5bc0de, #2f96b4);
  background-image: -ms-linear-gradient(top, #5bc0de, #2f96b4);
  background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#5bc0de), to(#2f96b4));
  background-image: -webkit-linear-gradient(top, #5bc0de, #2f96b4);
  background-image: -o-linear-gradient(top, #5bc0de, #2f96b4);
  background-image: linear-gradient(top, #5bc0de, #2f96b4);
