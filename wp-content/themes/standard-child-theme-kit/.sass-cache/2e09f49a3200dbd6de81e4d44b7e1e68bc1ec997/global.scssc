  filter: progid:dximagetransform.microsoft.gradient(enabled=false);*/

  filter:progid:DXImageTransform.Microsoft.Gradient(enabled='false');
  @include box-shadow(0);
  background-image:none;
  border:0;
  @include border-radius(0);
  padding: 9px 14px !important;
  @include adjust-font-size-to(15px);
  line-height: normal;
}

